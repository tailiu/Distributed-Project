var childProcess = require('child-proc');

function buildGitRepo(command){
  this.command = command;
}

buildGitRepo.prototype.build = function(aa){
  var ls = childProcess.execFile(aa, function (error, stdout, stderr) {
     if (error) {
       console.log(error.stack);
       console.log('Error code: '+stderr);
     } 
     console.log(stdout);
  });
}

module.exports = buildGitRepo;
